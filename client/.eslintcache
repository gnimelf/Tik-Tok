username: Auth.getProfile().data.username,\n        },\n      });\n\n      setTitle(\"\");\n      setDescription(\"\");\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const handleChange = (event) => {\n    const { name, value } = event.target;\n\n    if (name === \"description\" && value.length <= 280) {\n      setDescription(value);\n      setCharacterCount(value.length);\n    } else if (name === \"title\") {\n      setTitle(value);\n    }\n  };\n\n  return (\n    <div>\n      <h3>Add a Tic Tok product</h3>\n\n      {Auth.loggedIn() ? (\n        <>\n          <div className=\"col-12 col-lg-9\">\n            <input\n              name=\"title\"\n              placeholder=\"Here's a new title...\"\n              value={postTitle}\n              className=\"form-input w-100\"\n              style={{ lineHeight: \"1.5\", resize: \"vertical\" }}\n              onChange={handleChange}\n            ></input>\n          </div>\n          <p\n            className={`m-0 ${\n              characterCount === 280 || error ? \"text-danger\" : \"\"\n            }`}\n          >\n            Character Count: {characterCount}/280\n          </p>\n          <form\n            className=\"flex-row justify-center justify-space-between-md align-center\"\n            onSubmit={handleFormSubmit}\n          >\n            <div className=\"col-12 col-lg-9\">\n              <textarea\n                name=\"description\"\n                placeholder=\"Here's a new description...\"\n                value={postDescription}\n                className=\"form-input w-100\"\n                style={{ lineHeight: \"1.5\", resize: \"vertical\" }}\n                onChange={handleChange}\n              ></textarea>\n            </div>\n\n            <div className=\"col-12 col-lg-3\">\n              <button className=\"btn btn-primary btn-block py-3\" type=\"submit\">\n                Add Post\n              </button>\n            </div>\n            {error && (\n              <div className=\"col-12 my-3 bg-danger text-white p-3\">\n                {error.message}\n              </div>\n            )}\n          </form>\n        </>\n      ) : (\n        <p>\n          You need to be logged in to a post. Please{\" \"}\n          <Link to=\"/login\">login</Link> or <Link to=\"/signup\">signup.</Link>\n        </p>\n      )}\n    </div>\n  );\n};\n\nexport default PostForm;\n","C:\\Users\\rgall\\OneDrive\\Desktop\\Project3-Take2\\Tik-Tok\\client\\src\\components\\ReviewForm\\index.js",["74"],"import React, { useState } from 'react';\nimport { Link } from 'react-router-dom';\nimport { useMutation } from '@apollo/client';\n\nimport { ADD_REVIEW } from '../../utils/mutations';\n\nimport Auth from '../../utils/auth';\n\nconst ReviewForm = ({ postId }) => {\n  const [reviewText, setReviewText] = useState('');\n  const [characterCount, setCharacterCount] = useState(0);\n\n  const [addReview, { error }] = useMutation(ADD_REVIEW);\n\n  const handleFormSubmit = async (event) => {\n    event.preventDefault();\n\n    try {\n      const { data } = await addReview({\n        variables: {\n          postId,\n          reviewText,\n          username: Auth.getProfile().data.username,\n        },\n      });\n\n      setReviewText('');\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const handleChange = (event) => {\n    const { name, value } = event.target;\n\n    if (name === 'reviewText' && value.length <= 280) {\n      setReviewText(value);\n      setCharacterCount(value.length);\n    }\n  };\n\n  return (\n    <div>\n      <h4>What are your thoughts on this post?</h4>\n\n      {Auth.loggedIn() ? (\n        <>\n          <p\n            className={`m-0 ${\n              characterCount === 280 || error ? 'text-danger' : ''\n            }`}\n          >\n            Character Count: {characterCount}/280\n            {error && <span className=\"ml-2\">{error.message}</span>}\n          </p>\n          <form\n            className=\"flex-row justify-center justify-space-between-md align-center\"\n            onSubmit={handleFormSubmit}\n          >\n            <div className=\"col-12 col-lg-9\">\n              <textarea\n                name=\"reviewText\"\n                placeholder=\"Add your review...\"\n                value={reviewText}\n                className=\"form-input w-100\"\n                style={{ lineHeight: '1.5', resize: 'vertical' }}\n                onChange={handleChange}\n              ></textarea>\n            </div>\n\n            <div className=\"col-12 col-lg-3\">\n              <button className=\"btn btn-primary btn-block py-3\" type=\"submit\">\n                Add Review\n              </button>\n            </div>\n          </form>\n        </>\n      ) : (\n        <p>\n          You need to be logged in to add your review. Please{' '}\n          <Link to=\"/login\">login</Link> or <Link to=\"/signup\">signup.</Link>\n        </p>\n      )}\n    </div>\n  );\n};\n\nexport default ReviewForm;\n","C:\\Users\\rgall\\OneDrive\\Desktop\\Project3-Take2\\Tik-Tok\\client\\src\\components\\ReviewList\\index.js",[],"C:\\Users\\rgall\\OneDrive\\Desktop\\Project3-Take2\\Tik-Tok\\client\\src\\components\\PostList\\index.js",["75"],"import React from 'react';\nimport { Link } from 'react-router-dom';\n\nconst PostList = ({\n  posts,\n  title,\n  showTitle = true,\n  showUsername = true,\n}) => {\n  if (!posts.length) {\n    return <h3>No Posts Yet</h3>;\n  }\n\n  return (\n    <div>\n      {showTitle && <h3>{title}</h3>}\n      {posts &&\n        posts.map((post) => (\n          <div key={post._id} className=\"card mb-3\">\n            <h4 className=\"card-header bg-primary text-light p-2 m-0\">\n              {showUsername ? (\n                <Link\n                  className=\"text-light\"\n                  to={`/profiles/${post.username}`}\n                >\n                  {post.username} <br />\n                  <span style={{ fontSize: '1rem' }}>\n                    had this post on {post.createdAt}\n                  </span>\n                </Link>\n              ) : (\n                <>\n                  <span style={{ fontSize: '1rem' }}>\n                    You had this post on {post.createdAt}\n                  </span>\n                </>\n              )}\n            </h4>\n            <div className=\"card-body bg-light p-2\">\n            <img className='product-image' src={post.image}>\n            </img>\n              <a href={post.url}>  {post.title}</a>\n            </div>\n            <Link\n              className=\"btn btn-primary btn-block btn-squared\"\n              to={`/posts/${post._id}`}\n            >\n              Join the discussion on this thought.\n            </Link>\n          </div>\n        ))}\n    </div>\n  );\n};\n\nexport default PostList;\n",{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","replacedBy":"79"},{"ruleId":"80","severity":1,"message":"81","line":16,"column":15,"nodeType":"82","endLine":16,"endColumn":94},{"ruleId":"83","severity":1,"message":"84","line":41,"column":15,"nodeType":"85","messageId":"86","endLine":41,"endColumn":19},{"ruleId":"83","severity":1,"message":"84","line":19,"column":15,"nodeType":"85","messageId":"86","endLine":19,"endColumn":19},{"ruleId":"80","severity":1,"message":"81","line":40,"column":13,"nodeType":"82","endLine":40,"endColumn":61},"no-native-reassign",["87"],"no-negated-in-lhs",["88"],"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","no-unused-vars","'data' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]